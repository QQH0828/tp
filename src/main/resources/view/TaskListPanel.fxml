<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Label?>
<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" style="-fx-background-color: white; -fx-background-radius: 10; ">
    <Label fx:id="title" styleClass="title_label" text="Task List" />
    <Label fx:id="name" styleClass="heading_label" />
    <ListView fx:id="taskListView" VBox.vgrow="ALWAYS"/>
</VBox>
